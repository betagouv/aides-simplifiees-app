jei:
  titre: JEI

jei . eligibilite:
  titre: eligibilite au statut JEI
  valeur:
    toutes ces conditions:
      - estUnePME
      - Jeune Entreprise . conditionAge
      - jei . pourcentageRdCondition
      - capitalDetenu . aucune = non
      - activiteNouvelle

jei . pourcentageRdCondition:
  valeur:
    toutes ces conditions:
      - pourcentageRd >= 20

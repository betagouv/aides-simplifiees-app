
occasionnel-dix-voyages:
occasionnel-dix-voyages . eligibilite:
  valeur: oui
occasionnel-dix-voyages . resultat:
  texte: "<li>Carnet de dix tickets : <strong>{{ montant }}</strong></li>"

occasionnel-dix-voyages . montant:
  variations:
    - si: gratuité
      alors: 0 €
    - si:
        une de ces conditions:
          - militaire
          - serviceCivique
          - etudiant
          - familleNombreuse
          - tranche solidaire
      alors: 8.40 €
    - sinon: 16.80 €

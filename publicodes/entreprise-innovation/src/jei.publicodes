jei:
  titre: JEI

jei . eligibilite:
  titre: eligibilite au statut JEI
  toutes ces conditions:
    - estUnePME
    - jeuneEntreprise . conditionAge
    - jei . pourcentageRdCondition
    - capitalDetenu . aucune = non
    - activiteNouvelle

jei . pourcentageRdCondition:
  valeur:
    toutes ces conditions:
      - pourcentageRd >= 20

{
  "name": "aides-simplifiees-app",
  "type": "module",
  "version": "1.4.4",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "node bin/server.js",
    "dev": "node ace serve --hmr",
    "build": "node ace build --ignore-ts-errors",
    "prepare": "husky",
    "build:iframe-integration": "node scripts/build_iframe_integration.js",
    "detect:icons": "node scripts/detect_icons.js",
    "build:icons": "vue-dsfr-icons -s scripts/build_icons_collections.js -t inertia/icon_collections.ts",
    "build:publicodes": "node scripts/build_publicodes.js",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "typecheck": "pnpm run typecheck:backend && pnpm run typecheck:frontend && pnpm run typecheck:e2e-tests",
    "typecheck:backend": "tsc --noEmit",
    "typecheck:frontend": "tsc --noEmit -p inertia/tsconfig.json",
    "typecheck:e2e-tests": "tsc --noEmit -p tests/e2e/tsconfig.json",
    "detect_icons": "node scripts/detect_icons.js",
    "test": "node ace test",
    "test:a11y": "node ace test --files e2e/03_a11y",
    "clear:a11y": "rm -rf reports/accessibility",
    "a11y:report": "node scripts/accessibility_report.cjs reports/accessibility markdown",
    "a11y": "pnpm clear:a11y ; pnpm test:a11y && pnpm a11y:report",
    "test:coverage": "c8 node ace test",
    "coverage:report": "c8 report --reporter=html --reporter=text --reporter=lcov",
    "coverage:check": "c8 check-coverage",
    "coverage:open": "open coverage/index.html"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#publicodes/*": "./publicodes/"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.4.2",
    "@adonisjs/core": "^6.19.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "3.1.0",
    "@adonisjs/lucid": "^21.8.0",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.2.0",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "4.0.0",
    "@gouvfr/dsfr": "~1.14.0",
    "@gouvfr/dsfr-chart": "^2.0.4",
    "@gouvminint/vue-dsfr": "^8.6.0",
    "@iconify/vue": "^4.2.0",
    "@iframe-resizer/child": "^5.4.7",
    "@iframe-resizer/parent": "^5.4.7",
    "@inertiajs/vue3": "^2.0.17",
    "@publicodes/forms": "^0.6.0",
    "@publicodes/tools": "^1.7.2",
    "@vinejs/vine": "^3.0.1",
    "@vue/server-renderer": "^3.5.18",
    "@vueuse/core": "^13.6.0",
    "axios": "^1.11.0",
    "crisp-sdk-web": "^1.0.25",
    "crypto-js": "^4.2.0",
    "d3": "^7.9.0",
    "edge.js": "^6.3.0",
    "luxon": "^3.7.1",
    "marked": "^16.1.1",
    "pg": "^8.16.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "publicodes": "^1.9.0",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "vue": "^3.5.18",
    "vue-matomo": "^4.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.46.1",
    "@swc/core-linux-x64-gnu": "^1.13.2",
    "sass-embedded-linux-x64": "^1.89.2"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.1.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^1.4.1",
    "@antfu/eslint-config": "^5.0.0",
    "@axe-core/playwright": "^4.10.2",
    "@iconify-json/ri": "^1.2.5",
    "@japa/api-client": "^3.1.0",
    "@japa/assert": "^4.1.1",
    "@japa/browser-client": "^2.1.1",
    "@japa/expect": "^3.0.6",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.3.0",
    "@swc/core": "1.13.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/d3": "^7.4.3",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.1.0",
    "@types/sinon": "^17.0.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "axe-core": "^4.10.3",
    "c8": "^10.1.3",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^10.3.0",
    "hot-hook": "^0.4.0",
    "husky": "^9.1.7",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "sinon": "^21.0.0",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.8.3",
    "vite": "^6.1.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  }
}

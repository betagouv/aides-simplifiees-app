{
  "name": "aides-simplifiees-app",
  "type": "module",
  "version": "1.0.1",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build --ignore-ts-errors && pnpm build:iframe-integration",
    "build:iframe-integration": "vite build --config vite.iframe-integration.config.ts",
    "build:icons": "vue-dsfr-icons -s bin/icons.js -t inertia/icon_collections.ts",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#publicodes/*": "./publicodes/"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.3.1",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^3.1.0",
    "@adonisjs/lucid": "^21.6.0",
    "@adonisjs/session": "^7.5.1",
    "@adonisjs/shield": "^8.1.2",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^4.0.0",
    "@gouvfr/dsfr": "~1.13.0",
    "@gouvfr/dsfr-chart": "^2.0.3",
    "@gouvminint/vue-dsfr": "^8.4.0",
    "@iconify/vue": "^4.3.0",
    "@iframe-resizer/child": "^5.4.5",
    "@iframe-resizer/parent": "^5.4.5",
    "@inertiajs/vue3": "^2.0.6",
    "@publicodes/tools": "^1.7.2",
    "@vinejs/vine": "^3.0.0",
    "@vue/server-renderer": "^3.5.13",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.8.4",
    "crisp-sdk-web": "^1.0.25",
    "crypto-js": "^4.2.0",
    "edge.js": "^6.2.1",
    "luxon": "^3.6.1",
    "marked": "^15.0.8",
    "pg": "^8.14.1",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "publicodes": "^1.9.0",
    "reflect-metadata": "^0.2.2",
    "slugify": "^1.6.6",
    "vue": "^3.5.13",
    "vue-matomo": "^4.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.1",
    "@swc/core-linux-x64-gnu": "^1.11.22",
    "sass-embedded-linux-x64": "^1.87.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@antfu/eslint-config": "^4.12.0",
    "@iconify-json/ri": "^1.2.5",
    "@japa/assert": "^4.0.1",
    "@japa/browser-client": "^2.1.1",
    "@japa/expect": "^3.0.3",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.10.16",
    "@tsconfig/node22": "^22.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/luxon": "^3.6.0",
    "@types/node": "^22.13.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.0",
    "sass-embedded": "^1.86.2",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.7.3",
    "vite": "^6.1.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  }
}
